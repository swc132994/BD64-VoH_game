ACTOR Brutal_BloodSpot	//modelled
{
game Doom
Scale 0.5
radius 2
height 2
mass 1
Health 300
Speed 0
Gravity 1.0
Decal "BrutalBloodSplat"
-NOGRAVITY
-DONTSPLASH
-SOLID
-SHOOTABLE
+NOTELEPORT
+CLIENTSIDEONLY
+FORCEXYBILLBOARD
+MOVEWITHSECTOR
+FLOORCLIP
+NOTARGET
+NOBLOOD
+THRUACTORS
+MISSILE
+BOUNCEONFLOORS
BounceFactor 0.01
BounceCount 1
	states
	{
	Spawn:
		TNT1 A 0 NoDelay A_JumpIf(waterlevel > 1,"Splash")
		TNT1 A 0 A_CheckFloor("SpawnNormally")
		Goto Splash
	SpawnNormally:
		TNT1 A 0 A_QueueCorpse
		BPDL ABCD 0 A_Jump(64,"Rest")//Random Frame A B C D
		Goto Rest
	Rest:
		"####" "#" 0 ACS_NamedExecuteAlways("LowBloodCheckerACS")
		"####" "#" 800
		"####" "#" 0 A_JumpIfInventory("lowgraphicsmode",1,"Fade")
		Loop
	Fade:
		"####" "#" 2 A_FadeOut(0.01)
		Wait
	Splash:
		TNT1 A 3
		stop
	}
}

