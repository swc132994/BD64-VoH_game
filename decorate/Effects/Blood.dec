ACTOR TinyBlood
{
game Doom
scale 0.2
speed 7
health 0
radius 8
height 1
Gravity 0.7
Decal "BrutalBloodSplat"
-EXPLODEONWATER
+DONTSPLASH
+MISSILE
+CLIENTSIDEONLY
+NOTELEPORT
+NOBLOCKMAP
+FORCEXYBILLBOARD
+THRUACTORS
+CORPSE
BounceType "Doom"
BounceCount 0
BounceFactor 0.01
	States
	{
	Spawn:
		TNT1 A 0 NoDelay A_JumpIf(waterlevel > 1,"Splash")
		BLUD Z 500
		Stop
	Splash:
		TNT1 AAA 0 A_SpawnItemEx("UnderBlood1",0,0,0,0,0,0,0,SXF_TRANSFERTRANSLATION)
		TNT1 A 1
		stop
	Death:
		TNT1 A 0 A_PlaySound("blooddrop3",CHAN_BODY)
		XDT1 EFGHIJKL 2
		stop
	NoSpawn:
	Crush:
		TNT1 A 3
		Stop
	}
}

