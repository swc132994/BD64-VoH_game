ACTOR XDeath1
{
Scale 1.2
Radius 4
Height 4
Speed 10
Mass 1
SeeSound "misc/xdeath4"
DeathSound "misc/xdeath3"
Decal "BrutalBloodSuper"
DamageType "Blood"
+MISSILE
+NOBLOCKMAP
+NOTELEPORT
+MOVEWITHSECTOR
+CLIENTSIDEONLY
-DONTSPLASH
+THRUGHOST
+THRUACTORS
+FLOORCLIP
+ISMONSTER		//INFRARED
BounceType "Doom"
BounceCount 1
BounceFactor 1
	States
	{
	Spawn:
		BLOD A 0 NoDelay A_JumpIf(waterlevel > 1,"Underwater")
		BLOD A 4 A_CustomMissile("XDeath1Trail",0,0,random(0,360),2,random(0,360))
		Loop
	Death:
		TNT1 A 0 A_SpawnItemEx("Brutal_BloodSpot")
		XDT1 EFGHIJKL 2
		Stop
	Underwater:
	Splash:
		TNT1 A 0 ThrustThingZ(0,35,1,0)
		XDT1 AB 3 A_CustomMissile("Brutal_FlyingBloodTrail",0,0,random(0,360),2,random(0,360))
		TNT1 A 0 ThrustThingZ(0,35,1,0)
		XDT1 CD 3 A_CustomMissile("Brutal_FlyingBloodTrail",0,0,random(0,360),2,random(0,360))
		Loop
	}
}

