Actor KickAttack : FastProjectile
{
Damage(random(15,25))
DamageType "Kick"
Speed 32
Radius 18
Height 6
Alpha 0.6
RenderStyle Add
SeeSound "none"
DeathSound "none"
Decal "none"
Obituary "%o chewed on %k's foot."
+NOEXTREMEDEATH
	States
	{
	Spawn:
		TNT1 A 1
		Stop
	Death:
	XDeath:
		TNT1 A 0 A_CheckFloor("DeathOnGround")
		TNT1 A 0 A_PlaySound("FOOTWALL",CHAN_WEAPON)
		TNT1 AAAAAA 0 A_CustomMissile("MeleeSmoke",0,0,random(0,360),2,random(0,360))
		TNT1 A 0 Radius_Quake(2,6,0,5,0)
		TNT1 A 10
		Stop
	DeathOnGround:
		TNT1 A 0 A_PlaySound("FOOTWALL",CHAN_WEAPON)
		TNT1 AAAAAA 0 A_CustomMissile("MeleeSmoke",0,0,random(0,360),2,random(0,360))
		TNT1 A 0 Radius_Quake(2,6,0,5,0)
		TNT1 A 0 A_Explode(10,40,0)
		TNT1 A 10
		Stop
	}
}

